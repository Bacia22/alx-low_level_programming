#include "main.h"
/**
* _strcmp - compares pointers to two strings
* @s1: string 1
* @s2: strign 2
* Return: 0 is macthing, and ns1 - ns2 f not matching
*/
int _strcmp(char *s1, char *s2)
{
int i;
for (i = 0; s1[i] != '\0' || s2[i] != '\0'; i++)
{
if (s1[i] != s2[i])
{
return (s1[i] -s2[i]);
}
}
return (0);
}
